{
  "name": "cowchips-back",
  "version": "1.0.0",
  "description": "Backend for the cowchips4charity project",
  "main": "index.js",
  "scripts": {
    "test": "jest --detectOpenHandles --coverage",
    "test:watch": "jest --watch --detectOpenHandles",
    "serve": "nodemon --exec babel-node src/server.js",
    "serve:dist": "node dist/server.js test-prod-server",
    "start": "pm2-runtime start ecosystem.config.js --env production",
    "lint": "eslint .",
    "build": "babel src -d dist",
    "heroku-postbuild": "npm run build",
    "initdb": "mongorestore db-dump",
    "popdb": "babel-node scripts/populate.script",
    "pop-teams": "babel-node scripts/pop-teams.script",
    "test-batch-email": "babel-node scripts/test-batch-email.script",
    "stop": "kill -9 $(ps aux | grep [s]erver.js | awk '{print $2}')",
    "test:prod": "npm run serve:dist & sleep 2s && npm run stop",
    "make-template": "babel-node scripts/templates/make-template.script",
    "delete-template": "babel-node scripts/templates/delete-template.script",
    "get-template": "babel-node scripts/templates/get-template.script",
    "list-templates": "babel-node scripts/templates/list-templates.script",
    "test-email": "babel-node scripts/test-email.script"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bmeeder22/cowchips-back.git"
  },
  "author": "Connor Rust, Benjamin Meeder, Daniel Lev, Alexander Lev, Kenneth Ho, and Jack Boike",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bmeeder22/cowchips-back/issues"
  },
  "homepage": "https://github.com/bmeeder22/cowchips-back#readme",
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "aws-sdk": "^2.432.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "chance": "^1.0.18",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "joi": "^14.3.1",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^3.1.4",
    "mongoose": "^5.3.12",
    "pm2": "^3.2.9",
    "start-server-and-test": "^1.7.11",
    "stripe": "^6.20.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/mongoose": "^5.3.17",
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "chance": "^1.0.18",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^24.0.0",
    "nodemon": "^1.18.9",
    "regenerator-runtime": "^0.12.1",
    "transform-runtime": "0.0.0"
  }
}
